{
  "name": "wealth-mgr-mcpui-server",
  "version": "1.0.0",
  "description": "Wealth management agent - MCP-UI Server",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "mcp",
    "mcp-ui",
    "test-server",
    "model-context-protocol"
