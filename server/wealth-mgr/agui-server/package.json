{
  "name": "wealth-mgr-agui-server",
  "version": "1.0.0",
  "description": "Wealth management agent - AG-UI Server",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "ag-ui",
    "neuronkit",
    "sse",
    "agent",
    "test-server"
  ],
  "author": "eleung",
  "license": "MIT",
  "dependencies": {
    "@ag-ui/core": "^0.0.39",
    "@ag-ui/encoder": "^0.0.39",
    "@fastify/cors": "^10.0.1",
    "@modelcontextprotocol/sdk": "^1.22.0",
    "dotenv": "^16.4.7",
    "fastify": "^5.2.0",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "undici": "^7.2.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}